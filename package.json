    "@next/font": "^13.1.2",
    "@prisma/client": "^4.7.1",
    "@tanstack/react-query": "^4.3.8",
    "@trpc/client": "^10.9.0",
    "@trpc/next": "^10.9.0",
    "@trpc/react-query": "^10.9.0",
    "@trpc/server": "^10.9.0",
    "bcrypt": "^5.1.0",
    "clsx": "^1.1.1",
    "dotenv-cli": "^6.0.0",
    "next": "^13.0.0",
    "next-auth": "^4.18.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.42.1",
